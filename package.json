{
  "name": "cloud",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "precommit": "lint-staged",
    "get-types": "apollo-codegen introspect-schema http://10.4.1.42:3000/graphql --output schema.json && apollo-codegen generate src/**/*.gql --schema schema.json --target typescript --output src/base/schema.ts",
    "build": "webpack -p --config config/prod.config.js",
    "start": "webpack-dev-server --hot --inline --config config/dev.config.js"
  },
  "jest": {
    "roots": [
      "src"
    ],
    "mapCoverage": true,
    "testMatch": [
      "<rootDir>/src/**/*.test.tsx"
    ],
    "moduleFileExtensions": [
      "js",
      "ts",
      "tsx"
    ],
    "collectCoverageFrom": [
      "src/views/**"
    ],
    "setupFiles": [
      "<rootDir>/src/base/test-setup.js"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|less|scss)$": "identity-obj-proxy"
    },
    "transform": {
      "\\.(gql|graphql)$": "jest-transform-graphql",
      "^.+\\.(ts|tsx)$": "ts-jest/preprocessor.js"
    },
    "collectCoverage": false
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "apollo-client-preset": "0.1.0-1",
    "react": "^16.0.0",
    "react-apollo": "^1.4.16",
    "react-dom": "^16.0.0",
    "react-router-dom": "^4.2.2"
  },
  "lint-staged": {
    "*.test.tsx": [
      "jest"
    ],
    "*.{js,tsx,ts}": [
      "prettier --single-quote --trailing-comma es5 --write",
      "eslint",
      "git add"
    ]
  },
  "devDependencies": {
    "@types/css-modules": "^1.0.0",
    "@types/enzyme": "^2.8.8",
    "@types/graphql": "^0.11.4",
    "@types/jest": "^21.1.1",
    "@types/react": "^16.0.6",
    "@types/react-dom": "^15.5.4",
    "@types/react-router": "^4.0.15",
    "@types/react-router-dom": "^4.0.7",
    "apollo-codegen": "^0.16.5",
    "awesome-typescript-loader": "^3.2.3",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-jest": "^21.0.2",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-typescript": "^7.0.0-alpha.19",
    "css-loader": "^0.28.7",
    "cypress": "^1.0.1",
    "enzyme": "^3.0.0-beta.7",
    "enzyme-adapter-react-16": "^1.0.0",
    "eslint": "^4.7.2",
    "eslint-plugin-graphql": "^1.3.0",
    "eslint-plugin-react": "^7.4.0",
    "graphql-tag": "^2.4.2",
    "html-webpack-plugin": "^2.30.1",
    "husky": "^0.14.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^21.1.0",
    "jest-transform-graphql": "^2.1.0",
    "lint-staged": "^4.2.3",
    "node-sass": "^4.5.3",
    "prettier": "^1.7.0",
    "react-hot-loader": "^3.0.0-beta.7",
    "react-test-renderer": "^16.0.0",
    "sass-loader": "^6.0.6",
    "source-map-loader": "^0.2.1",
    "style-loader": "^0.18.2",
    "ts-jest": "^21.0.1",
    "typescript": "^2.5.2",
    "typescript-eslint-parser": "^8.0.0",
    "uglifyjs-webpack-plugin": "^0.4.6",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.8.2"
  }
}
